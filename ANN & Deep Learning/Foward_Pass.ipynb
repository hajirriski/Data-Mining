    "def forwardPass(inputs, weight, bias, activation = 'linear'):\n",
    "    w_sum = np.dot(inputs, weight) + bias\n",
    "\n",
    "    if activation is 'relu' :\n",
    "\t\t# ReLU Activation f(x) = max(0, x)\n",
    "\t    act = np.maximum(w_sum, 0)\n",
    "    else :\n",
    "\t\t# Linear Activation f(x) = x\n",
    "\t    act = w_sum\n",
    "\n",
    "    return act"
   ]
  }
